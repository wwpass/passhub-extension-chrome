{
  "name": "PassHub.net",
  "version": "1.6.0",
  "description": "Securely autofill login forms and payment details with PassHub password manager, built using zero-trust architecture.",
  "author": "WWPass",
  "homepage_url": "https://wwpass.com",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "webNavigation",
    "storage"
  ],
  "host_permissions": [
    "https://*/",
    "http://*/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": [
      "https://*.passhub.net/*",
      "https://*.passhub.ch/*",
      "https://*.passhub.us/*",
      "https://*.passhub.eu/*"
    ]
  },
  "action": {
    "default_icon": {
      "48": "images/passhub-logo-48.png",
      "128": "images/passhub-logo-128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "images/passhub-logo-48.png",
    "128": "images/passhub-logo-128.png"
  },
  "key": "",
  "manifest_version": 3
}